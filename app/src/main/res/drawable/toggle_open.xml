<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="20dp"/>
            <size
                android:width="36dp"
                android:height="20dp"/>
            <solid android:color="@color/myorder_item_bottom_layout"/>
            <stroke
                android:width="1dp"
                android:color="@color/regButton"/>
        </shape>
    </item>
    <item android:gravity="end">
        <shape android:shape="oval">
            <solid android:color="@color/regButton"/>
            <corners android:radius="20dp"/>
            <size
                android:width="20dp"
                android:height="20dp"/>
        </shape>
    </item>
</layer-list>